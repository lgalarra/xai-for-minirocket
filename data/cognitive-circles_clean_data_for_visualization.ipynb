    "   \"source\": [\n",
    "    \"import pandas as pd\\n\",\n",
    "    \"import sys\\n\",\n",
    "    \"sys.path.append(\\\"../code\\\")\\n\",\n",
    "    \"\\n\",\n",
    "    \"from utils import medoid_per_class, medoid\\n\",\n",
    "    \"\\n\",\n",
    "    \"# Read the two parquet files\\n\",\n",
    "    \"df1 = pd.read_hdf(\\\"cognitive-circles/df40participants.h5\\\")\\n\",\n",
    "    \"OUTPUT_FOLDER = 'cognitive-circles'\"\n",
    "   ]\n",
    "  },\n",
