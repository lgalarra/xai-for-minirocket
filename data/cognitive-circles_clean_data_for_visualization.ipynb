    "    }\n",
    "   },\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"meta_df = pd.DataFrame(metadata_dict)\\n\",\n",
    "    \"meta_df.T.to_csv(f\\\"{OUTPUT_FOLDER}/metadata.csv\\\", index=False)\\n\"\n",
    "   ]\n",
    "  },\n",
